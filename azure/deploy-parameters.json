{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerName": {
            "value": "vlol"
        },
        "location": {
            "value": "eastus"
        },
        "imageType": {
            "value": "Public"
        },
        "imageName": {
            "value": "hub.docker.com/_/hello-world"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "1"
        },
        "memory": {
            "value": "1.5"
        },
        "restartPolicy": {
            "value": "OnFailure"
        },
        "environmentVariables": {
            "value": [
                {
                    "name": "Test",
                    "value": "test"
                }
            ]
        },
        "ipAddressType": {
            "value": "Public"
        },
        "ports": {
            "value": [
                {
                    "port": "80",
                    "protocol": "TCP"
                },
                {
                    "port": "5000"
                }
            ]
        },
        "dnsNameLabel": {
            "value": "dev-vlol"
        }
    }
}